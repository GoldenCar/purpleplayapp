{
  "name": "com.platformpurple.purpleplay",
  "version": "2.0.6",
  "displayName": "Purple Player",
  "cordova": {
    "platforms": [
      "ios",
      "android"
    ],
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-crosswalk-webview": {
        "XWALK_VERSION": "23+",
        "XWALK_LITEVERSION": "xwalk_core_library_canary:17+",
        "XWALK_COMMANDLINE": "--disable-pull-to-refresh-effect",
        "XWALK_MODE": "embedded",
        "XWALK_MULTIPLEAPK": "true"
      },
      "cordova-plugin-spinner": {},
      "cordova-plugin-statusbar": {},
      "cordova-android-support-gradle-release": {
        "ANDROID_SUPPORT_VERSION": "24.+"
      },
      "cordova-plugin-file": {},
      "cordova-plugin-file-transfer": {},
      "cordova-plugin-zip": {},
      "cordova-plugin-camera": {},
      "cordova-plugin-code-push": {},
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-network-information": {},
      "cordova-plugin-splashscreen": {},
      "nl.x-services.plugins.backgroundaudio": {},
      "cordova-plugin-background-download": {},
      "com.mutatefile.plugin": {},
      "cordova-plugin-document-viewer": {},
      "cordova-plugin-file-opener2": {},
      "cordova-plugin-ionic-webview": {
        "ANDROID_SUPPORT_ANNOTATIONS_VERSION": "27.+"
      },
      "cordova-plugin-appavailability": {}
    }
  },
  "dependencies": {
    "code-push": "^2.0.6",
    "com.mutatefile.plugin": "./MutateFile_Plugin",
    "cordova-android": "~7.0.0",
    "cordova-android-support-gradle-release": "^1.4.7",
    "cordova-ios": "~4.5.5",
    "cordova-plugin-appavailability": "^0.4.2",
    "cordova-plugin-background-download": "git+https://github.com/sgrebnov/cordova-plugin-background-download.git",
    "cordova-plugin-camera": "^4.0.3",
    "cordova-plugin-code-push": "^1.11.17",
    "cordova-plugin-crosswalk-webview": "^2.4.0",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-dialogs": "^2.0.1",
    "cordova-plugin-document-viewer": "git+https://github.com/sitewaerts/cordova-plugin-document-viewer.git",
    "cordova-plugin-file": "^6.0.1",
    "cordova-plugin-file-opener2": "^2.2.0",
    "cordova-plugin-file-transfer": "^1.7.1",
    "cordova-plugin-inappbrowser": "^2.0.2",
    "cordova-plugin-ionic-webview": "git+https://github.com/platformpurple/cordova-plugin-ionic-webview.git",
    "cordova-plugin-network-information": "^2.0.1",
    "cordova-plugin-spinner": "^1.1.0",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "cordova-plugin-zip": "^3.1.0",
    "mutatefile_plugin": "file:MutateFile_Plugin",
    "nl.x-services.plugins.backgroundaudio": "^1.0.1"
  }
}